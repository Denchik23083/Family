@page "/parents/{ParentId:int}"


<div class="wrapper">    
    <div class="from_details">
        <IndividualParentsComponent Parent="Parent"/>
    </div>
    <div class="con">
        @foreach (var child in Children)
        {
            <div class="from_all_click" @onclick="() => RouteToChild(child.Id)">
                <IndividualChildrenComponent Child="child"/>
            </div>
        }
    </div>
</div>

