@page "/children"

<h3>Children</h3>

<table class="table">
    <thead>
    <tr>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Age</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var child in Children)
    {
        <IndividualChildrenComponent Child="child"/>
    }
    </tbody>
</table>
